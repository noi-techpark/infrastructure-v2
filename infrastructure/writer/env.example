# URL where to read integrated messages waiting to be written in the rawDataTable
INTERNAL_MQTT_URL=tcp://mosquittostorage:1883

# TOPIC where to read integrated messages waiting to be written in the rawDataTable
INTERNAL_MQTT_TOPIC=storage

# INTERNAL MQTT username, leave this setting empty if mosquitto
# allows anonymous access.
INTERNAL_MQTT_USER=

# INTERNAL MQTT username, leave this setting empty if mosquitto
# allows anonymous access.
INTERNAL_MQTT_PASSWORD=

# Connection string to the rawDataTable
# Follow documentation on https://www.mongodb.com/docs/manual/reference/connection-string/
MONGODB_CONNECTION_STRING=mongodb://mongodb1:27017